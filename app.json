
{
  "expo": {
    "name": "Aura",
    "slug": "aura",
    "version": "1.0.0",
    "platforms": ["android"],
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/112c7827-2c25-428e-b1c8-66d89163efd7.jpeg",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/112c7827-2c25-428e-b1c8-66d89163efd7.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "package": "com.auraapp.luxury",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/112c7827-2c25-428e-b1c8-66d89163efd7.jpeg",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ]
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SpaceMono-Regular.ttf",
            "./assets/fonts/SpaceMono-Bold.ttf",
            "./assets/fonts/SpaceMono-Italic.ttf",
            "./assets/fonts/SpaceMono-BoldItalic.ttf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/112c7827-2c25-428e-b1c8-66d89163efd7.jpeg",
          "color": "#D4AF37"
        }
      ]
    ]
  }
}
